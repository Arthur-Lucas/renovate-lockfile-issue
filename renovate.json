{
  "dependencyDashboard": true,
  "rangeStrategy": "pin",
  "lockFileMaintenance": {
    "enabled": true
  },
  "labels": [
    "dependencies",
    "javascript"
  ],
  "autoApprove": false,
  "enabledManagers": [
    "npm"
  ],
  "extends": [
    ":enableVulnerabilityAlerts",
    ":separateMajorReleases",
    ":combinePatchMinorReleases",
    ":ignoreUnstable",
    ":prImmediately",
    ":semanticPrefixFixDepsChoreOthers",
    ":updateNotScheduled",
    ":automergeDisabled",
    ":prHourlyLimitNone",
    ":prConcurrentLimitNone",
    "group:recommended",
    "helpers:disableTypesNodeMajor",
    "group:testNonMajor"
  ]
}
